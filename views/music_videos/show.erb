<% @band.music_videos.each do |video| %>
<iframe width="420" height="315" src="//www.youtube.com/embed/<%= video[:video_embed_url]
 %>" frameborder="0" allowfullscreen></iframe>

<form action="/bands/<%= @band.id %>/music_videos" method="POST">
  <input type="hidden" name= "_method" value="delete">
  <input type="submit" value="Delete This Video">
</form>
<% end %>
